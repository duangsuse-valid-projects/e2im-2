<manifest package="org.duangsuse.changeattr" xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application android:allowBackup="true" android:fullBackupContent="false" android:icon="@android:drawable/ic_menu_edit" android:label="@string/app_name" android:supportsRtl="true">
        <activity android:name=".ChangeAttr" android:label="@string/app_name" android:excludeFromRecents="true" android:theme="@style/Dialog">
            <intent-filter tools:ignore="AppLinkUrlError">
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="all/*"/>
            </intent-filter>
        </activity>
    </application>
</manifest>